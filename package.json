{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "Ask questions and get answers fast",
  "main": "app.js",
  "scripts": {
    "test": "nyc mocha --exit --require babel-core/register",
    "start-dev": "nodemon --exec babel-node /src/server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hacktivist123/stackoverflow-lite.git"
  },
  "author": "shedrack Akintayo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hacktivist123/stackoverflow-lite/issues"
  },
  "homepage": "https://github.com/hacktivist123/stackoverflow-lite#readme",
  "dependencies": {
    "babel-eslint": "^8.2.6",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^5.4.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "should": "^13.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2"
  }
}
